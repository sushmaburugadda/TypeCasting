public class Encapsulate 
{ 
    private String Name; 
    private int salary; 
    private int Age;
    public int getAge()  
    { 
      return Age; 
    } 
    public String getName()  
    { 
      return Name; 
    } 
    public int getsalary()  
    { 
       return salary; 
    } 
    public void setAge( int newAge) 
    { 
      Age = newAge; 
    } 
    public void setName(String newName) 
    { 
      Name = newName; 
    } 
    public void setsalary( int newsalary)  
    { 
      salary = newsalary; 
    } 
}

public class TestEncapsulate {
	public class Encapsulate 
	{ 
	    private String Name; 
	    private int Salary; 
	    private int Age;
	    public int getAge()  
	    { 
	      return Age; 
	    } 
	    public String getName()  
	    { 
	      return Name; 
	    } 
	    public int getsalary()  
	    { 
	       return Salary; 
	    } 
	    public void setAge( int newAge) 
	    { 
	      Age = newAge; 
	    } 
	    public void setName(String newName) 
	    { 
	      Name = newName; 
	    } 
	    public void setSalary( int newsalary)  
	    { 
	      Salary = newsalary; 
	    } 
	}

}
